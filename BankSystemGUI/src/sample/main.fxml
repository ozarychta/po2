<?xml version="1.0" encoding="UTF-8"?>
        <?import javafx.scene.control.Button?>
        <?import javafx.scene.control.CheckBox?>
        <?import javafx.scene.control.ChoiceBox?>
        <?import javafx.scene.control.TableColumn?>
        <?import javafx.scene.control.TableView?>
        <?import javafx.scene.control.TextField?>
        <?import javafx.scene.layout.ColumnConstraints?>
        <?import javafx.scene.layout.GridPane?>
        <?import javafx.scene.layout.RowConstraints?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<GridPane fx:controller="sample.Controller" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
          minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0"
          xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171">
<columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
</columnConstraints>
<rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
</rowConstraints>
<children>
    <Button mnemonicParsing="false" text="New Account" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" GridPane.columnSpan="5" GridPane.rowIndex="1" GridPane.rowSpan="6">
        <columns>
            <TableColumn  text="ClientID"  >
                <cellValueFactory>
                    <PropertyValueFactory property="clientID"/>
                </cellValueFactory>
            </TableColumn>

            <TableColumn  text="FirstName"  >
                <cellValueFactory>
                    <PropertyValueFactory property="firstName"/>
                </cellValueFactory>
            </TableColumn>

            <TableColumn  text="LastName"  >
                <cellValueFactory>
                    <PropertyValueFactory property="lastName"/>
                </cellValueFactory>
            </TableColumn>

            <TableColumn  text="Pesel"  >
                <cellValueFactory>
                    <PropertyValueFactory property="pesel"/>
                </cellValueFactory>
            </TableColumn>

            <TableColumn  text="Address"  >
                <cellValueFactory>
                    <PropertyValueFactory property="address"/>
                </cellValueFactory>
            </TableColumn>

            <TableColumn  text="Balance"  >
                <cellValueFactory>
                    <PropertyValueFactory property="balance"/>
                </cellValueFactory>
            </TableColumn>
        </columns>
    </TableView>
    <CheckBox mnemonicParsing="false" text="Filter" GridPane.columnIndex="1" GridPane.halignment="CENTER"
              GridPane.valignment="CENTER" />
    <ChoiceBox prefWidth="150.0" GridPane.columnIndex="3" />
    <Button mnemonicParsing="false" text="New Transaction" GridPane.columnIndex="4" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
    <TextField GridPane.columnIndex="2" GridPane.halignment="CENTER" GridPane.valignment="CENTER" />
</children>
</GridPane>